{
  "name": "ngx-spreadsheet",
  "version": "0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ngx-spreadsheet",
    "build:example": "ng build example",
    "publish": "rimraf ./dist && npm run build && npm publish ./dist/ngx-spreadsheet --access=public",
    "watch": "ng build ngx-spreadsheet --watch --configuration development",
    "ghpages": "rimraf dist/example && ng build example --configuration production --base-href \"https://helveg.github.io/ngx-spreadsheet/\" && ngh --dir=dist/example",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/common": "^20.0.0",
    "@angular/compiler": "^20.0.0",
    "@angular/core": "^20.0.0",
    "@angular/forms": "^20.0.0",
    "@angular/platform-browser": "^20.0.0",
    "@angular/platform-browser-dynamic": "^20.0.0",
    "@angular/router": "^20.0.0",
    "@ngrx/component": "^20.0.0",
    "deepmerge-ts": "^5.1.0",
    "ngx-spreadsheet": "^1.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.0.0",
    "@angular/cli": "^20.0.0",
    "@angular/compiler-cli": "^20.0.0",
    "@types/jasmine": "~3.8.0",
    "angular-cli-ghpages": "^1.0.7",
    "jasmine-core": "~3.8.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ng-packagr": "^20.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "^5.8.0"
  },
  "keywords": [
    "Angular",
    "Spreadsheet",
    "Lightweight"
  ],
  "author": "Eiji Hirakawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/e-hirakawa/ngx-spreadsheet"
  },
